	partition DateTable = calculated
		mode: import
		source =
				
				VAR StartDate = DATE(2025, 5, 1)
				VAR EndDate = TODAY()
				RETURN
				ADDCOLUMNS(
				    CALENDAR(StartDate, EndDate),
				    "Year", YEAR([Date]),
				    "Month", FORMAT([Date], "MMMM"),
				    "Month Number", MONTH([Date]),
				    "Day", DAY([Date]),
				    "Weekday", FORMAT([Date], "dddd")
